{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./lib", "./package.json"]
  },
  "opts": {
    "readme": "./README.md",
    "destination": "./docs/",
    "template": "./node_modules/jaguarjs-jsdoc",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "applicationName": "Entity Core",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "",
      "description": "",
      "keyword": ""
    },
    "linenums": false
  }
}
